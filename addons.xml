<addon id="plugin.program.jungle-team" name="KodiMaster Wizard" version="3.0.1.7a" provider-name="KodiMaster">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">KodiMaster Wizard</summary>
    <description lang="en">The Best Wizard for the Best Builds!  For help or info please visit http://www.kodimaster.com</description>
    <platform>all</platform>
  </extension>
</addon>
	
